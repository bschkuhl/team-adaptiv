<svg width="800" height="500" xmlns="http://www.w3.org/2000/svg">
  <rect width="100%" height="100%" fill="#f8fafc"/>
  <rect x="50" y="50" width="700" height="400" fill="#ffffff" stroke="#e2e8f0" stroke-width="2"/>
  <text x="400" y="100" text-anchor="middle" font-family="Arial, sans-serif" font-size="24" fill="#1a365d">Human-AI Collaboration</text>
  <text x="400" y="130" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" fill="#4a5568">Workplace Interaction Study</text>
  
  <!-- Human figure -->
  <g transform="translate(200, 200)">
    <circle cx="0" cy="-20" r="15" fill="#fbbf24" stroke="#f59e0b" stroke-width="2"/>
    <rect x="-10" y="-5" width="20" height="30" fill="#3b82f6" rx="5"/>
    <rect x="-8" y="25" width="6" height="20" fill="#1f2937"/>
    <rect x="2" y="25" width="6" height="20" fill="#1f2937"/>
    <rect x="-20" y="0" width="8" height="15" fill="#fbbf24"/>
    <rect x="12" y="0" width="8" height="15" fill="#fbbf24"/>
    <text x="0" y="65" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#374151">Human User</text>
  </g>
  
  <!-- AI representation -->
  <g transform="translate(600, 200)">
    <rect x="-20" y="-30" width="40" height="40" fill="#6366f1" rx="8"/>
    <circle cx="-8" cy="-20" r="3" fill="#ffffff"/>
    <circle cx="8" cy="-20" r="3" fill="#ffffff"/>
    <rect x="-10" y="-10" width="20" height="3" fill="#ffffff" rx="1"/>
    <rect x="-15" y="10" width="30" height="20" fill="#4f46e5" rx="5"/>
    <text x="0" y="50" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#374151">AI System</text>
  </g>
  
  <!-- Interaction arrows and communication -->
  <path d="M280 200 Q400 160 520 200" stroke="#22c55e" stroke-width="3" fill="none" marker-end="url(#arrowhead2)"/>
  <path d="M520 220 Q400 260 280 220" stroke="#ef4444" stroke-width="3" fill="none" marker-end="url(#arrowhead2)"/>
  
  <text x="400" y="175" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#16a34a">Input/Query</text>
  <text x="400" y="275" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#dc2626">Response/Result</text>
  
  <!-- Trust meter -->
  <rect x="120" y="350" width="200" height="20" fill="#f1f5f9" stroke="#64748b" stroke-width="1" rx="10"/>
  <rect x="120" y="350" width="140" height="20" fill="#22c55e" rx="10"/>
  <text x="120" y="345" font-family="Arial, sans-serif" font-size="12" fill="#374151">Trust Level: 70%</text>
  
  <!-- Performance metrics -->
  <rect x="480" y="350" width="200" height="20" fill="#f1f5f9" stroke="#64748b" stroke-width="1" rx="10"/>
  <rect x="480" y="350" width="170" height="20" fill="#3b82f6" rx="10"/>
  <text x="480" y="345" font-family="Arial, sans-serif" font-size="12" fill="#374151">Efficiency: 85%</text>
  
  <!-- Collaboration stages -->
  <rect x="100" y="400" width="120" height="30" fill="#ddd6fe" stroke="#8b5cf6" stroke-width="1" rx="5"/>
  <text x="160" y="420" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#6b21a8">Initial Skepticism</text>
  
  <rect x="250" y="400" width="120" height="30" fill="#fed7d7" stroke="#f56565" stroke-width="1" rx="5"/>
  <text x="310" y="420" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#c53030">Learning Phase</text>
  
  <rect x="400" y="400" width="120" height="30" fill="#c6f6d5" stroke="#48bb78" stroke-width="1" rx="5"/>
  <text x="460" y="420" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#2f855a">Acceptance</text>
  
  <rect x="550" y="400" width="120" height="30" fill="#bee3f8" stroke="#4299e1" stroke-width="1" rx="5"/>
  <text x="610" y="420" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#2b6cb0">Collaboration</text>
  
  <!-- Arrow marker definition -->
  <defs>
    <marker id="arrowhead2" markerWidth="10" markerHeight="7" refX="9" refY="3.5" orient="auto">
      <polygon points="0 0, 10 3.5, 0 7" fill="#22c55e"/>
    </marker>
  </defs>
</svg>
