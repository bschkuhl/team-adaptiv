<svg width="800" height="500" xmlns="http://www.w3.org/2000/svg">
  <rect width="100%" height="100%" fill="#f8fafc"/>
  <rect x="50" y="50" width="700" height="400" fill="#ffffff" stroke="#e2e8f0" stroke-width="2"/>
  <text x="400" y="100" text-anchor="middle" font-family="Arial, sans-serif" font-size="24" fill="#1a365d">Advanced Data Visualization</text>
  <text x="400" y="130" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" fill="#4a5568">Multidimensional Framework</text>
  
  <!-- Main visualization area -->
  <rect x="100" y="160" width="600" height="250" fill="#f8fafc" stroke="#e2e8f0" stroke-width="1"/>
  
  <!-- 3D-like scatter plot -->
  <g opacity="0.8">
    <!-- Data points with varying sizes and colors -->
    <circle cx="200" cy="200" r="8" fill="#ef4444"/>
    <circle cx="250" cy="180" r="12" fill="#f97316"/>
    <circle cx="320" cy="220" r="6" fill="#eab308"/>
    <circle cx="380" cy="190" r="10" fill="#22c55e"/>
    <circle cx="450" cy="210" r="15" fill="#3b82f6"/>
    <circle cx="520" cy="170" r="9" fill="#8b5cf6"/>
    <circle cx="580" cy="230" r="7" fill="#ec4899"/>
    
    <circle cx="180" cy="280" r="11" fill="#ef4444"/>
    <circle cx="240" cy="300" r="8" fill="#f97316"/>
    <circle cx="300" cy="260" r="13" fill="#eab308"/>
    <circle cx="400" cy="290" r="6" fill="#22c55e"/>
    <circle cx="480" cy="270" r="10" fill="#3b82f6"/>
    <circle cx="550" cy="310" r="12" fill="#8b5cf6"/>
    <circle cx="620" cy="250" r="9" fill="#ec4899"/>
    
    <!-- Connecting lines showing relationships -->
    <path d="M200 200 L250 180 L320 220" stroke="#64748b" stroke-width="1" fill="none" opacity="0.5"/>
    <path d="M380 190 L450 210 L520 170" stroke="#64748b" stroke-width="1" fill="none" opacity="0.5"/>
    <path d="M300 260 L400 290 L480 270" stroke="#64748b" stroke-width="1" fill="none" opacity="0.5"/>
  </g>
  
  <!-- Dimensional axes -->
  <line x1="120" y1="400" x2="680" y2="400" stroke="#374151" stroke-width="2"/>
  <line x1="120" y1="400" x2="120" y2="170" stroke="#374151" stroke-width="2"/>
  <line x1="120" y1="400" x2="150" y2="380" stroke="#374151" stroke-width="2"/>
  
  <!-- Axis labels -->
  <text x="400" y="430" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#374151">Dimension X</text>
  <text x="90" y="285" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#374151" transform="rotate(-90, 90, 285)">Dimension Y</text>
  <text x="135" y="375" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#374151">Dimension Z</text>
  
  <!-- Legend -->
  <rect x="120" y="440" width="560" height="40" fill="#f1f5f9" stroke="#cbd5e0" stroke-width="1"/>
  <text x="130" y="455" font-family="Arial, sans-serif" font-size="12" fill="#374151">Dataset Categories:</text>
  
  <circle cx="250" cy="460" r="5" fill="#ef4444"/>
  <text x="260" y="465" font-family="Arial, sans-serif" font-size="10" fill="#374151">Genomics</text>
  
  <circle cx="330" cy="460" r="5" fill="#22c55e"/>
  <text x="340" y="465" font-family="Arial, sans-serif" font-size="10" fill="#374151">Climate</text>
  
  <circle cx="400" cy="460" r="5" fill="#3b82f6"/>
  <text x="410" y="465" font-family="Arial, sans-serif" font-size="10" fill="#374151">Physics</text>
  
  <circle cx="470" cy="460" r="5" fill="#8b5cf6"/>
  <text x="480" y="465" font-family="Arial, sans-serif" font-size="10" fill="#374151">Social</text>
  
  <!-- Interactive elements indicators -->
  <rect x="580" y="180" width="100" height="60" fill="#ffffff" stroke="#3182ce" stroke-width="2" rx="5"/>
  <text x="630" y="200" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#3182ce">Interactive</text>
  <text x="630" y="215" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#3182ce">Controls</text>
  <circle cx="620" cy="225" r="3" fill="#3182ce"/>
  <circle cx="630" cy="225" r="3" fill="#3182ce"/>
  <circle cx="640" cy="225" r="3" fill="#3182ce"/>
</svg>
