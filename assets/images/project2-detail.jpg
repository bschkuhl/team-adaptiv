<svg width="800" height="500" xmlns="http://www.w3.org/2000/svg">
  <rect width="100%" height="100%" fill="#f8fafc"/>
  <rect x="50" y="50" width="700" height="400" fill="#ffffff" stroke="#e2e8f0" stroke-width="2"/>
  <text x="400" y="100" text-anchor="middle" font-family="Arial, sans-serif" font-size="24" fill="#1a365d">Cognitive Load Measurement</text>
  <text x="400" y="130" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" fill="#4a5568">Digital Interface Framework</text>
  
  <!-- Three cognitive load types -->
  <rect x="100" y="170" width="150" height="100" fill="#fef3c7" stroke="#f59e0b" stroke-width="2" rx="8"/>
  <text x="175" y="200" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" fill="#92400e">Intrinsic Load</text>
  <text x="175" y="220" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#b45309">Task Complexity</text>
  <text x="175" y="240" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#b45309">Subject Matter</text>
  
  <rect x="325" y="170" width="150" height="100" fill="#fee2e2" stroke="#ef4444" stroke-width="2" rx="8"/>
  <text x="400" y="200" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" fill="#dc2626">Extraneous Load</text>
  <text x="400" y="220" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#dc2626">Poor Design</text>
  <text x="400" y="240" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#dc2626">Distractions</text>
  
  <rect x="550" y="170" width="150" height="100" fill="#dcfce7" stroke="#22c55e" stroke-width="2" rx="8"/>
  <text x="625" y="200" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" fill="#16a34a">Germane Load</text>
  <text x="625" y="220" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#16a34a">Schema Building</text>
  <text x="625" y="240" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#16a34a">Learning Process</text>
  
  <!-- Measurement tools -->
  <rect x="150" y="320" width="500" height="80" fill="#f1f5f9" stroke="#64748b" stroke-width="1" rx="8"/>
  <text x="400" y="345" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" fill="#334155">Measurement Tools</text>
  <text x="250" y="370" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#64748b">Eye Tracking</text>
  <text x="400" y="370" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#64748b">EEG Monitoring</text>
  <text x="550" y="370" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#64748b">Performance Metrics</text>
  
  <!-- Chart representation -->
  <rect x="100" y="430" width="600" height="2" fill="#e2e8f0"/>
  <rect x="200" y="420" width="80" height="12" fill="#3182ce" opacity="0.6"/>
  <rect x="350" y="410" width="60" height="22" fill="#ef4444" opacity="0.6"/>
  <rect x="500" y="425" width="100" height="7" fill="#22c55e" opacity="0.6"/>
</svg>
