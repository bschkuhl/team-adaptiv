<svg width="800" height="500" xmlns="http://www.w3.org/2000/svg">
  <rect width="100%" height="100%" fill="#f8fafc"/>
  <rect x="50" y="50" width="700" height="400" fill="#ffffff" stroke="#e2e8f0" stroke-width="2"/>
  <text x="400" y="100" text-anchor="middle" font-family="Arial, sans-serif" font-size="24" fill="#1a365d">Sustainable Computing Analysis</text>
  <text x="400" y="130" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" fill="#4a5568">Energy Efficiency in Distributed Systems</text>
  
  <!-- Energy consumption chart -->
  <rect x="120" y="160" width="300" height="200" fill="#f8fafc" stroke="#e2e8f0" stroke-width="1"/>
  <text x="270" y="180" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" fill="#374151">Energy Consumption (kWh)</text>
  
  <!-- Before optimization -->
  <rect x="160" y="200" width="40" height="120" fill="#ef4444"/>
  <text x="180" y="340" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#374151">Before</text>
  <text x="180" y="190" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#ef4444">2400</text>
  
  <!-- After optimization -->
  <rect x="260" y="240" width="40" height="80" fill="#22c55e"/>
  <text x="280" y="340" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#374151">After</text>
  <text x="280" y="230" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#22c55e">1750</text>
  
  <!-- Improvement indicator -->
  <path d="M200 210 Q230 190 260 250" stroke="#fbbf24" stroke-width="3" fill="none" marker-end="url(#arrowhead3)"/>
  <text x="230" y="215" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#f59e0b">-27%</text>
  
  <!-- Carbon footprint visualization -->
  <rect x="480" y="160" width="220" height="200" fill="#f0fdf4" stroke="#22c55e" stroke-width="2"/>
  <text x="590" y="180" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" fill="#15803d">Carbon Footprint Reduction</text>
  
  <!-- CO2 representation -->
  <g transform="translate(520, 220)">
    <circle cx="0" cy="0" r="25" fill="#ef4444" opacity="0.3"/>
    <text x="0" y="5" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#dc2626">CO₂</text>
    <text x="0" y="45" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#374151">2.4 tons</text>
  </g>
  
  <text x="590" y="220" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" fill="#22c55e">→</text>
  
  <g transform="translate(660, 220)">
    <circle cx="0" cy="0" r="15" fill="#22c55e" opacity="0.6"/>
    <text x="0" y="5" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#15803d">CO₂</text>
    <text x="0" y="35" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#374151">1.3 tons</text>
  </g>
  
  <text x="590" y="320" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" fill="#15803d">45% Reduction</text>
  
  <!-- Renewable energy integration -->
  <rect x="120" y="380" width="580" height="50" fill="#fef3c7" stroke="#f59e0b" stroke-width="1"/>
  <text x="130" y="400" font-family="Arial, sans-serif" font-size="12" fill="#92400e">Renewable Energy Integration:</text>
  
  <!-- Solar panels representation -->
  <rect x="350" y="390" width="15" height="8" fill="#3b82f6"/>
  <rect x="370" y="390" width="15" height="8" fill="#3b82f6"/>
  <rect x="390" y="390" width="15" height="8" fill="#3b82f6"/>
  <text x="377" y="415" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#1e40af">Solar: 35%</text>
  
  <!-- Wind energy representation -->
  <g transform="translate(480, 395)">
    <line x1="0" y1="0" x2="0" y2="-10" stroke="#22c55e" stroke-width="2"/>
    <path d="M0 -10 L-5 -15 M0 -10 L5 -15 M0 -10 L0 -20" stroke="#22c55e" stroke-width="1"/>
  </g>
  <text x="480" y="415" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#15803d">Wind: 25%</text>
  
  <!-- Efficiency metrics -->
  <rect x="550" y="390" width="60" height="8" fill="#8b5cf6"/>
  <text x="580" y="415" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#7c3aed">Grid: 40%</text>
  
  <!-- Arrow marker definition -->
  <defs>
    <marker id="arrowhead3" markerWidth="10" markerHeight="7" refX="9" refY="3.5" orient="auto">
      <polygon points="0 0, 10 3.5, 0 7" fill="#fbbf24"/>
    </marker>
  </defs>
</svg>
